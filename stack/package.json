  "sideEffects": false,
  "scripts": {
    "remove-classnames": "node scripts/remove-classnames.js",
    "pre-build": "run-p pre-build:* || true",
    "pre-build:css": "tailwindcss -m -i ./styles/app.css -o app/tailwind/tailwind.css --minify",
    "build": "npm run pre-build && run-p build:*",
    "build:server": "esbuild --platform=node --format=cjs ./server/* --outdir=build --sourcemap=external",
    "build:remix": "remix build --sourcemap",
    "build:storybook": "npm run storybook:build",
    "build:strapi": "npm run build --prefix ./strapi",
    "pre-dev": "run-p pre-build build:server build:remix",
    "dev": "npm run pre-dev && run-p dev:*",
    "dev:strapi": "npm run develop --prefix ./strapi",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./mocks ./build/server.js",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/tailwind/tailwind.css --minify",
    "clean": "rm -r build public/build playwright-report storybook-static app/tailwind/tailwind.css && npm run --prefix strapi clean",
    "build+start": "npm run build && npm run start",
    "secrets": "cp .envrc.example .envrc && direnv allow",
    "devs-test": "npm run secrets && npm run test && npm run test:integration && npm run format && npm run validate",
    "test-and-run": "npm run devs-test && npm run dev",
    "first-run": "npm i && npm run test-and-run",
    "start": "run-p start:*",
    "start:server": "cross-env NODE_ENV=production node ./build/server.js",
    "start:strapi": "npm run start --prefix ./strapi",
    "storybook": "run-p dev:css storybook:start",
    "storybook:start": "start-storybook -h localhost -p 6006",
    "storybook:build": "build-storybook",
    "storybook:test": "test-storybook --maxWorkers=2 --url http://localhost:3000/_/storybook",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . && prettier -c .",
    "setup": "prisma migrate dev && prisma db seed",
    "test": "vitest --watch false",
    "test:integration": "vitest --passWithNoTests -c integration-vitest.config.ts",
    "percy": "percy",
    "pretest:e2e:run": "npm run build",
    "test:e2e": "npx playwright test",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e",
    "postinstall": "patch-package",
    "setup-helper": "cd scripts && ./setup.sh"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
